{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "strict": true,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "lib": ["es2015"]
    },
    "exclude": [
        "node_modules"
    ]
}
